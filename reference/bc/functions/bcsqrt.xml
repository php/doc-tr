<refentry xml:id="function.bcsqrt" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>bcsqrt</refname>
  <refpurpose>Keyfi duyarlıklı bir sayının karekök alma işlemini yapar
  </refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>bcsqrt</methodname>
   <methodparam><type>string</type><parameter>terim</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>olcek</parameter></methodparam>
  </methodsynopsis>
  <para>
   <parameter>terim</parameter>&apos;in karekökünü döndürür.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>terim</parameter></term>
     <listitem>
      <para>
       Dizge olarak terim.
      </para>
     </listitem>
    </varlistentry>
    &bc.scale.description;
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Dizge olarak karekökü veya <parameter>terim</parameter> negatifse &null;
   döndürür.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title><function>bcsqrt</function> örneği</title>
   <programlisting role="php">
<![CDATA[
<?php

echo bcsqrt('2', 3); // 1.414

?>
]]>
   </programlisting>
  </example>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>bcpow</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

