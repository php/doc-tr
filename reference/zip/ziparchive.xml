<!-- {{{ ZipArchive properties -->
  <section xml:id="ziparchive.props">
   &reftitle.properties;
   <variablelist>
    <varlistentry xml:id="ziparchive.props.lastid">
     <term><varname>lastId</varname></term>
     <listitem>
      <para>Son eklenen girdinin (dosya veya dizin) indis değeri.
      PHP 8.0.0 ve PECL zip 1.18.0 itibariyle kullanılabilir.</para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="ziparchive.props.status">
     <term><varname>status</varname></term>
     <listitem>
      <para>Zip Arşivinin durumu.
       PHP 8.0.0 ve PECL zip 1.18.0 itibariyle kapalı arşiv için
       kullanılabilir.</para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="ziparchive.props.statussys">
     <term><varname>statusSys</varname></term>
     <listitem>
      <para>Zip Arşivinin sistem durumu.
       PHP 8.0.0 ve PECL zip 1.18.0 itibariyle kapalı arşiv için
       kullanılabilir.</para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="ziparchive.props.numfiles">
     <term><varname>numFiles</varname></term>
     <listitem>
      <para>Arşivdeki dosya sayısı</para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="ziparchive.props.filename">
     <term><varname>filename</varname></term>
     <listitem>
      <para>Dosya sistemindeki dosya ismi</para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="ziparchive.props.comment">
     <term><varname>comment</varname></term>
     <listitem>
      <para>Arşivle ilgili açıklama</para>
     </listitem>
    </varlistentry>
   </variablelist>
  </section>
<!-- }}} -->
 </partintro>


 &reference.zip.entities.ziparchive;

