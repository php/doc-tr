<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-setimageclipmask">
 <refnamediv>
  <refname>Imagick::setImageClipMask</refname>
  <refpurpose>Resim kırpma maskesini tanımlar</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>Imagick::setImageClipMask</methodname>
   <methodparam><type>Imagick</type><parameter>kırpıcı_resim</parameter></methodparam>
  </methodsynopsis>

  <para>
   Resmin kırpma maskesi olarak başka bir nesneyi tanımlar.
   &imagick.method.available.0x636;
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>kırpıcı_resim</parameter></term>
     <listitem>
      <para>
       Kırpma maskesini içeren <classname>Imagick</classname> nesnesi.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &imagick.imagick.throws;
  </para>
 </refsect1>

</refentry>

