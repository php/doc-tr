<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-transverseimage">
 <refnamediv>
  <refname>Imagick::transverseImage</refname>
  <refpurpose>Resmin yataydaki yansımasını oluşturur</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>Imagick::transverseImage</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Pikselleri 270 derece döndürerek ve y eksenine göre yansıtarak resmin
   yataydaki yansımasını oluşturur.
   &imagick.method.available.0x629;
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>Imagick::transposeImage</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

