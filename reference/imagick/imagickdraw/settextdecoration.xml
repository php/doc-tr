<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagickdraw-settextdecoration">
 <refnamediv>
  <refname>ImagickDraw::setTextDecoration</refname>
  <refpurpose>Bir karakter süslemesi tanımlar</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ImagickDraw::setTextDecoration</methodname>
   <methodparam><type>int</type><parameter>süs</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Metin çizilirken uygulanacak süsleme belirtilir.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>süs</parameter></term>
     <listitem>
      <para>
       Süsleme sabitlerinden (<constant>DECORATION_</constant>)  biri.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>

