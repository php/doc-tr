<refentry xml:id="function.posix-getuid" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>posix_getuid</refname>
  <refpurpose>Geçerli sürecin gerçek kullanıcı kimliğini döndürür</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>posix_getuid</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Geçerli sürecin gerçek kullanıcı kimliğini döndürür.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Kullanıcı kimliğini bir tamsayı olarak döndürür.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>- <function>posix_getuid</function> örneği</title>
    <programlisting role="php">
<![CDATA[
<?php
echo posix_getuid(); //10000
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>posix_getpwuid</function></member>
    <member>POSIX getuid(2) kılavuz sayfası</member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

