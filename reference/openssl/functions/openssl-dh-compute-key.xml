<refentry xml:id="function.openssl-dh-compute-key" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>openssl_dh_compute_key</refname>
  <refpurpose>Uzak ve yerel özet hesaplama anahtarları için paylaşımlı
  anahtarı hesaplar</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>openssl_dh_compute_key</methodname>
   <methodparam><type>string</type><parameter>genanh</parameter></methodparam>
   <methodparam><type>resource</type><parameter>özetanh</parameter></methodparam>
  </methodsynopsis>

  &warn.undocumented.func;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>genanh</parameter></term>
     <listitem>
      <para>
       Genel anahtar.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>özetanh</parameter></term>
     <listitem>
      <para>
       Özet hesaplama anahtarı.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   İşlem başarısız olursa &false; yoksa hesaplanan anahtar dizgesi döner.
  </para>
 </refsect1>

</refentry>

