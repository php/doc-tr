<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.openssl-x509-free">
 <refnamediv>
  <refname>openssl_x509_free</refname>
  <refpurpose>Sertifika özkaynağını serbest bırakır</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>openssl_x509_free</methodname>
   <methodparam><type>resource</type><parameter>x509sert</parameter></methodparam>
  </methodsynopsis>
  <para>
   <parameter>x509sert</parameter> ile belirtilen bellek özkaynağını serbest bırakır.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>x509sert</parameter></term>
     <listitem>
      <para>
       <function>openssl_x509_read</function> tarafından döndürülen bir X509
       sertifika özkaynağı.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>

