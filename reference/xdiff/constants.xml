<appendix xml:id="xdiff.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry>
   <term>
    <constant>XDIFF_PATCH_NORMAL</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     <function>xdiff_string_patch</function> ve
     <function>xdiff_file_patch</function> işlevlerinin yamayı özgün dosyaya
     uygulayıp dosyanın yeni sürümünü elde etmek amacıyla kullanılacağını
     belirtir. Bu öntanımlı işlem kipidir.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>XDIFF_PATCH_REVERSE</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     <function>xdiff_string_patch</function> ve
     <function>xdiff_file_patch</function> işlevlerinin yamayı yeni sürüme
     uygulayıp dosyanın özgün sürümünü elde etmek amacıyla kullanılacağını
     belirtir.
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
</appendix>

