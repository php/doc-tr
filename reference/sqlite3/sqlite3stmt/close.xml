 <refnamediv>
  <refname><classname>SQLite3Result</classname>::close</refname>
  <refpurpose>Hazır deyimi kapatır</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>SQLite3Stmt::close</methodname>
   <void />
  </methodsynopsis>
  <para>
   Hazır deyimi kapatır.
  </para>
  <note>
   <simpara>
    Bu <classname>SQLite3Stmt</classname> çalıştırılarak elde edilen tüm
    <classname>SQLite3Result</classname>lar deyim kapandığında geçersiz olur.
   </simpara>
  </note>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; döner.
  </para>
 </refsect1>

</refentry>

