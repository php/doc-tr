<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="messageformatter.getlocale">
 <refnamediv>
  <refname>MessageFormatter::getLocale</refname>
  <refname>msgfmt_get_locale</refname>
  <refpurpose>Biçemleyicinin oluşturulduğu yereli döndürür</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <para>
   Nesne yönelimli kullanım
  </para>
  <methodsynopsis>
   <type>string</type>
   <methodname>MessageFormatter::getLocale</methodname>
   <void />
  </methodsynopsis>
  <para>
   Yordamsal kullanım
  </para>
  <methodsynopsis>
   <type>string</type>
   <methodname>msgfmt_get_locale</methodname>
   <methodparam><type>NumberFormatter</type><parameter>biçemleyici</parameter></methodparam>
  </methodsynopsis>
  <para>
   Biçemleyicinin oluşturulduğu yereli döndürür.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>biçemleyici</parameter></term>
     <listitem>
      <para>
       Biçemleyici nesne
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>


 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Yerel ismi
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>- <function>msgfmt_get_locale</function> örneği</title>
   <programlisting role="php">
<![CDATA[
<?php
$fmt = msgfmt_create('tr_TR', "Sayı: {0,number}");
echo msgfmt_get_locale($fmt);
?>
]]>
   </programlisting>
  </example>
  <example>
   <title>- Nesne yönelimli örnek</title>
   <programlisting role="php">
<![CDATA[
<?php
$fmt = new MessageFormatter('tr_TR', "Sayı: {0,number}");
echo $fmt->getLocale();
?>
]]>
   </programlisting>
  </example>
  &example.outputs;
  <screen>
<![CDATA[
tr_TR
]]>
  </screen>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>msgfmt_create</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>
