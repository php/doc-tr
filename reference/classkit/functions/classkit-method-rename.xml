<refentry xml:id="function.classkit-method-rename" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>classkit_method_rename</refname>
  <refpurpose>Verilen yöntemin ismini devingen olarak değiştirir</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>classkit_method_rename</methodname>
   <methodparam><type>string</type><parameter>sinifadi</parameter></methodparam>
   <methodparam><type>string</type><parameter>yontemadi</parameter></methodparam>
   <methodparam><type>string</type><parameter>yeniisim</parameter></methodparam>
  </methodsynopsis>
  &note.classkit.selfmanipulation;
  &warn.experimental.func;
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>sinifadi</parameter></term>
     <listitem>
      <para>
       Yeniden isimlendirilecek yöntemin sınıf adı
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>yontemadi</parameter></term>
     <listitem>
      <para>
       Yeniden isimlendirilecek yöntemin adı
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>yeniisim</parameter></term>
     <listitem>
      <para>
       Yöntemin yeni ismi
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
 <!-- No changelog: it's only in PECL. -->
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>classkit_method_rename</function> örneği</title>
    <programlisting role="php">
<![CDATA[
<?php
class Example {
    function foo() {
        return "foo!\n";
    }
}

// 'foo' yöntemini 'bar' olarak isimlendir
classkit_method_rename(
    'Example',
    'foo',
    'bar'
);

// yöntem çıktısı
echo Example::bar();
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
foo!
]]>
    </screen>
   </example>
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>classkit_method_add</function></member>
    <member><function>classkit_method_copy</function></member>
    <member><function>classkit_method_redefine</function></member>
    <member><function>classkit_method_remove</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

