<refentry xml:id="function.ftp-cdup" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ftp_cdup</refname>
  <refpurpose>Üst dizine geçer</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ftp_cdup</methodname>
   <methodparam><type>resource</type><parameter>ftp</parameter></methodparam>
  </methodsynopsis>
  <para>
   Üst dizine geçer.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>ftp</parameter></term>
     <listitem>
      <para>
       FTP bağlantısının bağlantı tanıtıcısı.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>- <function>ftp_cdup</function> örneği</title>
    <programlisting role="php">
<![CDATA[
<?php

// Temel SSL bağlantısını kuralım
$conn_id = ftp_ssl_connect($ftp_server);

// kullanıcı adı ve parola ile oturum açalım
$login_result = ftp_login($conn_id, $ftp_user_name, $ftp_user_pass);

// çalışma dizinini html yapalım
ftp_chdir($conn_id, 'html');

echo ftp_pwd($conn_id); // /html

// üst dizine dönelim
if (ftp_cdup($conn_id)) {
  echo "Üst dizine sorunsuzca dönüldü\n";
} else {
  echo "Üst dizine dönülemedi\n";
}

echo ftp_pwd($conn_id); // /

ftp_close($conn_id);
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>ftp_chdir</function></member>
    <member><function>ftp_pwd</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

