<refentry xml:id="domxpath.registernamespace" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DOMXPath::registerNamespace</refname>
  <refpurpose>Belirtilen isim alanını <classname>DOMXPath</classname> nesnesi
  ile ilişkilendirir</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>DOMXPath::registerNamespace</methodname>
   <methodparam><type>string</type><parameter>önek</parameter></methodparam>
   <methodparam><type>string</type><parameter>uri</parameter></methodparam>
  </methodsynopsis>
  <para>
   <parameter>önek</parameter> ve <parameter>uri</parameter> ile belirtilen
   isim alanını <classname>DOMXPath</classname> nesnesi ile ilişkilendirir.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>önek</parameter></term>
     <listitem>
      <para>
       İsim alanı öneki.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>uri</parameter></term>
     <listitem>
      <para>
       İsim alanını betimleyen adres.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

