<refentry xml:id="domelement.setidattributens" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DOMElement::setIdAttributeNS</refname>
  <refpurpose>İsmi ve isim alanı belirtilen özniteliği bir ID özniteliği olarak
  tanımlar</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>DOMElement::setIdAttributeNS</methodname>
   <methodparam><type>string</type><parameter>uri</parameter></methodparam>
   <methodparam><type>string</type><parameter>isim</parameter></methodparam>
   <methodparam><type>bool</type><parameter>id_olarak</parameter></methodparam>
  </methodsynopsis>
  <para>
   <parameter>uri</parameter> ile belirtilen isim alanındaki
   <parameter>isim</parameter> özniteliğini bir ID özniteliği haline getirir.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>uri</parameter></term>
     <listitem>
      <para>
       Özniteliğin isim alanını betimleyen adres.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>isim</parameter></term>
     <listitem>
      <para>
       <literal>önek:öznitelik</literal> biçeminde özniteliğin ismi.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>id_olarak</parameter></term>
     <listitem>
      <para>
       <parameter>isim</parameter> özniteliği bir ID özniteliği olarak
       bildiriliyorsa &true;, aksi takdirde &false; olmalıdır.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   <variablelist>
    <varlistentry>
     <term><constant>DOM_NO_MODIFICATION_ALLOWED_ERR</constant></term>
     <listitem>
      <para>Düğüm salt okunursa oluşur.</para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><constant>DOM_NOT_FOUND</constant></term>
     <listitem>
      <para>
       <parameter>isim</parameter>, bu düğümün bir özniteliği değilse oluşur.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>DOMDocument::getElementById</methodname></member>
    <member><methodname>DOMElement::setIdAttribute</methodname></member>
    <member><methodname>DOMElement::setIdAttributeNode</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

