 <refnamediv>
  <refname>Fiber::__construct</refname>
  <refpurpose>Yeni bir Fiber örneği oluşturur</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <constructorsynopsis>
   <modifier>public</modifier> <methodname>Fiber::__construct</methodname>
   <methodparam><type>callable</type><parameter>callback</parameter></methodparam>
  </constructorsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>callback</parameter></term>
    <listitem>
     <para>
      Fiber başlatılırken kullanılacak geri çağırım işlevi.
      İşleve aktarılan değiştirgeler <methodname>Fiber::start</methodname>
      yöntemine aktarılan değiştirgelerdir.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

</refentry>
